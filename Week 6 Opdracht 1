public class opdracht1 {


    public static void main(String[] args) {
        new opdracht1().run();

    }

    public void run() {

        methodeHuisje();
    }


    public void methodeHuisje() {
        System.out.println("   +   \n" +
                "  + +  \n" +
                " +   + \n" +
                "+-----+\n" +
                "|  _  |\n" +
                "| | | |\n" +
                "| | | |\n" +
                "+_+_+_|");
    }


}

public class opdracht8 {

    public static void main(String[] args) {

        int[] numbers = {4, 2, 6, 1};
        int[] numbers2 = numbers.clone();


        for (int i = 0; i < numbers.length; i++) {
            System.out.println("Array 1: " +numbers[i]);
            System.out.println("Array 2: " +numbers2[i]);
        }

    }
}
